create table ligne_commande(num_cmde INT, ref_prod VARCHAR(5), quantit√© INT, prix_Vente INT, constraint pk_ligne_commande PRIMARY KEY (num_cmde,ref_prod), constraint fk_lgn_cmde_commande FOREIGN KEY (num_cmde) REFERENCES commande (num_cmde) on delete set NULL on update CASCADE, constraint fk_lgn_cmde_produit FOREIGN KEY (ref_prod) REFERENCES produit (ref_prod) on delete set NULL on update CASCADE);